{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "karaokems-api",
    "main": "src/index.ts",
    "compatibility_date": "2025-04-26",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "observability": {
        "enabled": true,
        "head_sampling_rate": 1
    },
    "dev": {
        "port": 3000
    },
    "assets": {
        "directory": "./public",
        "binding": "ASSETS"
    },
    "vars": {
        "ENV": "development",
        "DOMAIN": "karaokems.com",
        "RATE_WINDOW_MS": 300000,
        "RATE_LIMIT": 100
    },
    "routes": []
}